{
  "name": "mipa",
  "version": "0.0.1",
  "description": "A strategic coordination app",
  "productName": "mipa",
  "author": "",
  "private": true,
  "scripts": {
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "dev": "cross-env QENV=development quasar dev",
    "combined": "cross-env QENV=development+test quasar dev",
    "build": "cross-env QENV=production quasar build",
    "build:pwa": "cross-env QENV=production quasar build -m pwa",
    "test:unit": "jest --updateSnapshot",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "test:unit:ui": "majestic",
    "test:e2e": "cross-env E2E_TEST=true start-test 'quasar dev' http-get://localhost:8080 'cypress open'",
    "test:e2e:ci": "cross-env E2E_TEST=true start-test 'quasar dev' http-get://localhost:8080 'cypress run'",
    "serve:test:lighthouse": "quasar serve test/lighthouse/reports/ --port 8789",
    "audit:snyk": "snyk test --json > ./test/audits/snyk.json",
    "audit:node_modules": "yarn audit --json > ./test/audits/node_modules_audit.json",
    "audit:licenses": "nlf --summary detail > ./test/audits/licenses.txt",
    "audit:lighthouse": "lighthouse http://localhost:3000 view --output-path=./test/lighthouse/reports/index.html --config-path=./test/lighthouse/configs/full-config.js"
  },
  "dependencies": {
    "@quasar/extras": "^1.8.2",
    "core-js": "3.0.0",
    "d3": "^5.11.0",
    "d3-context-menu": "^1.1.1",
    "d3-selection-multi": "^1.0.1",
    "eslint": "^6.2.2",
    "firebase": "^6.4.2",
    "mathjs": "^6.1.0",
    "object-sizeof": "^1.5.1",
    "quasar": "^1.12.8",
    "vue-google-charts": "^0.3.2",
    "vue-i18n": "^8.21.1",
    "vue-mathjax": "0.0.11",
    "vuex-map-fields": "^1.4.0",
    "vuexfire": "^3.1.2",
    "xstate": "^4.10.0"
  },
  "devDependencies": {
    "@quasar/app": "2.0.0",
    "@quasar/quasar-app-extension-qenv": "^1.0.3",
    "@quasar/quasar-app-extension-testing": "^1.0.0",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^2.0.1",
    "@quasar/quasar-app-extension-testing-quality": "^1.0.0-beta.8",
    "@quasar/quasar-app-extension-testing-security": "^1.0.0-alpha.8",
    "@quasar/quasar-app-extension-testing-security-antivuln": "^1.0.0-alpha.2",
    "@quasar/quasar-app-extension-testing-unit-jest": "^2.0.0",
    "cross-env": "^7.0.2",
    "devtron": "^1.4.0",
    "electron": "^7.2.4",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-jest": "^23.8.2",
    "majestic": "^1.2.24",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.3.1",
    "wallaby-vue-compiler": "^1.0.3",
    "workbox-webpack-plugin": "5.0.0"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ]
}
